{
  "name": "anava-cloud-installer",
  "version": "2.1.5",
  "description": "One-click installer for Anava IoT Security Platform on Google Cloud",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:security": "jest --testPathPattern=security",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "version:update": "node scripts/update-version.js",
    "version:patch": "npm version patch --no-git-tag-version && node scripts/update-version.js $(node -p \"require('./package.json').version\") PATCH",
    "version:minor": "npm version minor --no-git-tag-version && node scripts/update-version.js $(node -p \"require('./package.json').version\") ENHANCED",
    "version:major": "npm version major --no-git-tag-version && node scripts/update-version.js $(node -p \"require('./package.json').version\") MAJOR"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@react-oauth/google": "^0.12.1",
    "axios": "^1.6.0",
    "framer-motion": "^10.0.0",
    "next": "14.2.30",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.1.0",
    "typescript": "^5.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.0"
  }
}
